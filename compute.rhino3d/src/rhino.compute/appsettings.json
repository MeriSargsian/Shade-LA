{
  "Serilog": {
    "Using": [ "SeriLog.Sinks.Console", "Serilog.Sinks.File"],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "Rhino.Compute"
    }
  },

  "compute": {
    "spawn_children": true,
    "child_count": 1,
    "load_grasshopper": true,
    "create_headless_doc": true,
    "port": 6500,
    "child_spawn_wait_ms": 8000
  },

  "AllowedHosts": "*"
}
